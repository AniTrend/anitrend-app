<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <ListPreference
        android:defaultValue="4"
        android:entries="@array/pref_titles_menu_entries"
        android:entryValues="@array/pref_values_menu_entries"
        android:key="@string/pref_key_startup_page"
        android:title="@string/pref_title_startup_page" />

    <ListPreference
        android:defaultValue="master"
        android:entries="@array/pref_titles_channel_entries"
        android:entryValues="@array/pref_values_channel_entries"
        android:key="@string/pref_key_update_channel"
        android:title="@string/pref_title_update_channel" />

    <SwitchPreference
        android:defaultValue="false"
        android:title="@string/pref_title_amoled_theme"
        android:summaryOff="@string/pref_amoled_theme_summary_off"
        android:summaryOn="@string/pref_amoled_theme_summary_on"
        android:key="@string/pref_key_amoled_theme" />



</PreferenceScreen>